        <!-- Custody Clearance Sequence -->
        <record id="seq_finance_approval" model="ir.sequence">
            <field name="name">Custody Clearance No.</field>
            <field name="code">custody.clearance.sequence</field>
            <field name="prefix">CLR/%(year)s/</field>
