<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <data noupdate="1">
        <!-- Finance Approval Sequence -->
        <record id="seq_finance_approval" model="ir.sequence">
            <field name="name">Finance Approval No.</field>
            <field name="code">finance.approval.sequence</field>
            <field name="prefix">FAP/%(year)s/</field>
            <field name="padding">4</field>
            <field name="company_id" eval="False"/>
            <field name="implementation">no_gap</field>
        </record>
    </data>
</odoo>